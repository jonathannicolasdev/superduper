{
  "name": "superdupergallery",
  "description": "Super Duper Gallery",
  "version": "0.1.0",
  "author": "Jonathan Nicolas",
  "homepage": "https://superdupergallery.com",
  "repository": "https://github.com/jonathannicolasdev/superdupergallery",
  "bugs": "https://github.com/jonathannicolasdev/superdupergallery/issues",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:serve": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server-express.js --watch ./server-express.js",
    "build": "nr prisma:generate && remix build",
    "start": "cross-env NODE_ENV=production node ./server-express.js",
    "tailwind:view": "tailwind-config-viewer -p 7000 -o",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:push:seed": "prisma db push && prisma db seed",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma db push --force-reset",
    "check": "run-p check:*",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "eslint": "DEBUG=eslint:cli-engine nr check:lint",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./app/styles/*.css",
    "stylelint:fix": "stylelint --fix ./app/styles/*.css",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "prettier": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "clean": "rm -rf build public/build",
    "postinstall": "nr clean && nr prisma:generate"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@balavishnuvj/remix-seo": "^1.0.2",
    "@fontsource/archivo": "^4.5.11",
    "@fontsource/chivo-mono": "^4.5.1",
    "@fontsource/inter": "^4.5.15",
    "@headlessui/react": "^1.7.13",
    "@prisma/client": "^4.11.0",
    "@radix-ui/react-accessible-icon": "^1.0.2",
    "@radix-ui/react-accordion": "^1.1.1",
    "@radix-ui/react-alert-dialog": "^1.0.3",
    "@radix-ui/react-aspect-ratio": "^1.0.2",
