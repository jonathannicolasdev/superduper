    "@remix-run/eslint-config": "^1.14.1",
    "@remix-run/serve": "^1.14.0",
    "@swc/core": "^1.3.39",
    "@swc/helpers": "^0.4.14",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.15.0",
    "@types/nprogress": "^0.2.0",
    "@types/pluralize": "^0.0.29",
    "@types/prettyjson": "^0.0.30",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
