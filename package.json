    "@remix-run/vercel": "^1.14.1",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@vercel/node": "^2.9.12",
    "axios": "^1.3.4",
