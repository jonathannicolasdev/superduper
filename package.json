    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "prisma": "^4.11.0",
    "react-refresh": "^0.14.0",
    "remix-themes": "^1.2.1",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
