{
  "name": "superdupergallery",
  "description": "Super Duper Gallery",
  "version": "0.1.0",
  "author": "Jonathan Nicolas",
  "homepage": "https://superdupergallery.com",
  "repository": "https://github.com/jonathannicolasdev/superdupergallery",
  "bugs": "https://github.com/jonathannicolasdev/superdupergallery/issues",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "typecheck": "tsc",
    "tailwind:view": "tailwind-config-viewer -p 7000 -o",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:push:seed": "prisma db push && prisma db seed",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma db push --force-reset",
    "prisma:validate": "prisma validate",
    "check": "run-p check:*",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "eslint": "DEBUG=eslint:cli-engine nr check:lint",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./app/styles/*.css",
    "stylelint:fix": "stylelint --fix ./app/styles/*.css",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,mdx,json}\" --cache",
    "prettier": "prettier --write \"**/*.{ts,tsx,mdx,json}\" --cache",
    "clean": "rm -rf build public/build",
    "postinstall": "nr clean && nr prisma:generate"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
